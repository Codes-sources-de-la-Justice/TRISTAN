  "private": true,
  "dependencies": {
    "@dataesr/react-dsfr": "^0.8.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@vitejs/plugin-react": "^1.1.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "axios": "^0.24.0",
    "dagre": "^0.8.5",
    "konva": "^8.3.2",
    "leaflet": "^1.7.1",
    "moment": "^2.29.1",
    "pa11y": "^6.0.1",
    "pspdfkit": "^2021.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-konva": "^17.0.2-5",
    "react-leaflet": "^3.2.2",
    "react-simple-tree-menu": "^1.1.18",
    "use-image": "^1.0.8",
    "vite": "^2.7.3",
    "web-vitals": "^1.0.1",
    "wouter": "^2.8.0-alpha.1"
  },
  "scripts": {
    "start": "vite src/frontend",
    "build": "vite build src/frontend",
    "test": "web-test-runner \"**/*.test.jsx\" --coverage --config src/frontend/web-test-runner.config.js --node-resolver",
    "test:jest": "vite-jest src/frontend",
    "test:a11y": "pa11y"
  },
  "jest": {
    "preset": "vite-jest",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/frontend/setupTests.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@web/test-runner": "^0.13.22",
    "chai": "^4.3.4",
    "fast-glob": "^3.2.7",
    "jest": "^27.4.3",
    "jest-canvas-mock": "^2.3.1",
    "jest-environment-jsdom": "^27.4.4",
    "parseurl": "^1.3.3",
    "prettier": "^2.4.1",
    "send": "^0.17.1",
    "vite-jest": "^0.1.0",
    "vite-web-test-runner-plugin": "^0.1.0"
  }
